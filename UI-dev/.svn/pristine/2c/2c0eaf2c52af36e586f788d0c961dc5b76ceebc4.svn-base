@-webkit-keyframes slideFadeIn {
	0% { -webkit-transform: translate( 0px, 0px); opacity:0; }
	100% { -webkit-transform: translate(0px, 0px); opacity:1;}
}

@-webkit-keyframes slideFadeIn {
	0% { -webkit-transform: translate( 15px, 0px); opacity:0; }
	100% { -webkit-transform: translate(0px, 0px); opacity:1;}
}

@-webkit-keyframes slideFadeOut {
	0% { -webkit-transform: translate(0px, 0px); opacity:1 }
	100% { -webkit-transform: translate(15px, 0px); opacity:0;}
}

.questionGroup:first-child h1,
.questionGroup:first-child h2,
.questionGroup:first-child h3,
.questionGroup:first-child p,
.questionGroup:first-child .question_nav,
.questionGroup:first-child select.ans_button
{
    -webkit-animation-name: noAnimation!important;
    -webkit-animation-duration: 0s!important;
	-webkit-transform-origin:50% 50%!important;
	-webkit-animation-iteration-count: 1!important;
	-webkit-animation-timing-function: linear!important;
}

.questionGroup h1,
.questionGroup h2,
.questionGroup h3,
.questionGroup p,
.questionGroup .question_nav,
.questionGroup select.ans_button,
.questionnaire_answer_container,
.personal_questionnaire .questionGroup,
.matches_container *,
.shakefade{
	-webkit-animation-name: slideFadeIn;
	-webkit-animation-duration: 0.4s;
	-webkit-transform-origin:50% 50%;
	-webkit-animation-iteration-count: 1;
	-webkit-animation-timing-function: linear;
}

.questionGroup .questionGroup{
    margin-top:30px;
}

.questionGroup p a{
    color: #00C6CE;
}

.shakefadeOut{
	-webkit-animation-name: slideFadeOut;
	-webkit-animation-duration: 0.4s;
	-webkit-transform-origin:50% 50%;
	-webkit-animation-iteration-count: 1;
	-webkit-animation-timing-function: linear;
}

.modal {
  text-align: center;
}

@media screen and (min-width: 768px) { 
  .modal:before {
    display: inline-block;
    vertical-align: middle;
    content: " ";
    height: 100%;
  }
}

.modal-dialog {
  display: inline-block;
  text-align: left;
  vertical-align: middle;
}
